package pack2;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;
import java.util.StringTokenizer;

public class Library {
	
	public static void main(String[] args) throws Exception {
		
		File file = new File("c:\\work\\전국도서관표준데이터.csv");
		FileReader fr = new FileReader(file);
		BufferedReader br = new BufferedReader(fr);
		
		while(true) {
			String ss = br.readLine();
			StringTokenizer tok = new StringTokenizer(ss, ",");
			if(ss==null) break;
			
			String name = tok.nextToken(); //도서관명
			String area1 = tok.nextToken(); //시도명
			String area2 = tok.nextToken(); //시군구명
			String kind = tok.nextToken(); //도서관유형
			String not = tok.nextToken(); //휴관일
			String wds = tok.nextToken(); //평일운영시작시각
			String wde = tok.nextToken(); //평일운영종료시각
			String sds = tok.nextToken(); //토요일운영시작시각
			String sde = tok.nextToken(); //토요일운영종료시각
			String hds = tok.nextToken(); //공휴일운영시작시각
			String hde = tok.nextToken(); //공휴일운영종료시각
			String seat = tok.nextToken(); //열람좌석수
			String many1 = tok.nextToken(); //자료수(도서)
			String many2 = tok.nextToken(); //자료수(연속간행물)
			String many3 = tok.nextToken(); //자료수(비도서)
			String brr = tok.nextToken(); //대출가능권수
			String brrday = tok.nextToken(); //대출가능일수
			String road = tok.nextToken(); //소재지도로명주소
			String aname = tok.nextToken(); //운영기관명
			String call = tok.nextToken(); //도서관전화번호
			String ground1 = tok.nextToken(); //부지면적
			String ground2 = tok.nextToken(); //건물면적
			String http = tok.nextToken(); //홈페이지주소
			String we = tok.nextToken(); //위도
			String ae = tok.nextToken(); //경도
			String day = tok.nextToken(); //데이터기준일자
			
			System.out.println(ss);
		}
		br.close();
		fr.close();
		
		/*System.out.println("도서관명" + "\t" + "시도명" + "\t" 
		+ "도서관유형" + "\t" + "휴관일" + "\t" + "자료수(도서)"+ "\t" + "도서관 전화");*/
		
		while(true) {
			name
		}
		
	}

}
